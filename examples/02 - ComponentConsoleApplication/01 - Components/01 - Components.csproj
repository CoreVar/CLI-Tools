  <ItemGroup>
    <ProjectReference Include="..\..\..\src\CommandLineInterface\CommandLineInterface.csproj" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference
      Include="..\..\..\src\CommandLineInterface.SourceGenerator\CommandLineInterface.SourceGenerator.csproj"
      OutputItemType="Analyzer"
      ReferenceOutputAssembly="false"/>
  </ItemGroup>

</Project>
